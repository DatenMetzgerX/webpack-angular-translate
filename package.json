{
  "name": "webpack-angular-translate",
  "version": "3.0.2",
  "description": "Webpack plugin that extracts the translation-ids with the default texts.",
  "repository": "https://github.com/MichaReiser/webpack-angular-translate",
  "main": "dist/index.js",
  "typings": "./i18n.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "pretest": "tsc",
    "test": "mocha './test/**/*.specs.js'",
    "start": "mocha --watch './test/**/*.specs.js'",
    "prepublish": "tsc",
    "coverage": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha './test/**/*.specs.js'",
    "coveralljs": "npm run coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf coverage"
  },
  "author": "Micha Reiser <micha@famreiser.ch>",
  "license": "MIT",
  "dependencies": {
    "acorn": "^6.1.0",
    "ast-types": "^0.12.2",
    "cheerio": "^1.0.0-rc.2",
    "escodegen": "^1.11.0",
    "htmlparser2": "^3.10.1",
    "loader-utils": "^1.2.3",
    "source-map": "^0.7.3",
    "webpack": "^4.29.4",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.5",
    "@types/cheerio": "^0.22.10",
    "@types/escodegen": "^0.0.6",
    "@types/estree": "^0.0.39",
    "@types/htmlparser2": "^3.7.31",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^11.9.4",
    "@types/webpack": "^4.4.24",
    "@types/webpack-sources": "^0.1.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "deep-extend": "^0.6.0",
    "html-loader": "^0.5.5",
    "istanbul": "^0.4.2",
    "memfs": "^2.15.2",
    "mocha": "^5.2.0",
    "sinon": "^7.2.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "unionfs": "^4.0.0"
  }
}
